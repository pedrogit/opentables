{
  "name": "opentables",
  "version": "1.0.0",
  "description": "Restful server of configurable and formattable tables",
  "main": "server/index.js",
  "proxy": "http://localhost:3001/",
  "scripts": {
    "test-template": "cross-env NODE_ENV=development nodemon --inspect --exec \"mocha common/test/testTemplateParser.js --timeout 0\"",
    "test-schema": "cross-env NODE_ENV=development nodemon --inspect --exec \"mocha server/test/testSchemaValidator.js --timeout 0\"",
    "test-utils": "cross-env NODE_ENV=development nodemon --inspect --exec \"mocha common/test/testUtils.js --timeout 0\"",
    "test-filter": "cross-env NODE_ENV=development nodemon --inspect --exec \"mocha common/test/testFilter.js --timeout 0\"",
    "test-simpleJSON": "cross-env NODE_ENV=development nodemon --inspect --exec \"mocha common/test/testSimpleJSON.js --timeout 0\"",
    "test-permissions": "cross-env NODE_ENV=development nodemon --inspect --exec \"mocha common/test/testPermissions.js --timeout 0\"",
    "test-routes": "cross-env NODE_ENV=development nodemon --inspect --exec \"mocha server/test/testRoutes.js --timeout 0\"",
    "test-all": "cross-env NODE_ENV=development nodemon --inspect --exec \"mocha common/test/testTemplateParser.js server/test/testSchemaValidator.js common/test/testUtils.js common/test/testFilter.js common/test/testSimpleJSON.js common/test/testPermissions.js --timeout 0\"",
    "test-components": "cross-env NODE_ENV=development nodemon --inspect --exec \"mocha client/src/components/components.test.js --timeout 0\"",
    "start": "node ./server/index.js",
    "debug-server": "cross-env NODE_ENV=development nodemon --nolazy --inspect --watch ./common --watch ./server/src --watch ./server/index.js ./server/index.js",
    "cypress:open": "cypress open --browser chrome --e2e",
    "cypress:open-edge": "cypress open --browser edge --e2e",
    "cypress:run": "cypress run --browser chrome --config video=false,screenshotOnRunFailure=false",
    "debug-client": "webpack serve --open --config ./webpack.dev.js",
    "build": "webpack --config ./webpack.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pedrogit/opentables.git"
  },
  "keywords": [
    "restful",
    "tables",
    "database"
  ],
  "author": "Pierre Racine",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/pedrogit/opentables/issues"
  },
  "homepage": "https://github.com/pedrogit/opentables#readme",
  "dependencies": {
    "@babel/core": "^7.17.12",
    "@babel/preset-env": "^7.17.12",
    "@babel/preset-react": "^7.17.12",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.8.0",
    "@mui/material": "^5.8.0",
    "ajv": "^8.7.1",
    "axios": "^0.26.1",
    "babel-loader": "^8.2.5",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "crypto-browserify": "^3.12.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "js-cookie": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongodb": "^4.1.0",
    "process": "^0.11.10",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-google-recaptcha": "^2.1.0",
    "react-jsx-parser": "^1.29.0",
    "serve": "^12.0.1",
    "set-cookie-parser": "^2.4.8",
    "stream-browserify": "^3.0.0",
    "terser-webpack-plugin": "^5.3.3",
    "url-regex": "^5.0.0",
    "util": "^0.12.4",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "cookie": "^0.5.0",
    "cross-env": "^7.0.3",
    "csv-parse": "^5.1.0",
    "csvtojson": "^2.0.10",
    "cypress": "^10.0.2",
    "cypress-plugin-tab": "^1.0.5",
    "eslint": "^8.16.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-webpack-plugin": "^3.1.1",
    "mocha": "^9.0.2",
    "nodemon": "^2.0.10",
    "prettier": "^2.6.2",
    "react-test-renderer": "^18.1.0",
    "webpack-dev-server": "^4.9.0",
    "webpack-merge": "^5.8.0"
  }
}
